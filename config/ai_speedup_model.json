  "calculation_formula": {
    "description": "How to calculate AI-assisted time from manual time",
    "steps": [
      "1. Get base_manual_hours from LOC estimate (LOC * human_seconds_per_line / 3600)",
      "2. Calculate avg_tech_difficulty from technology_difficulty.json",
      "3. Get base_speedup from technology difficulty tier",
      "4. Adjust for project_size (coherence_penalty)",
      "5. Adjust for task_complexity (boilerplate vs specialized)",
      "6. Final: ai_hours = base_manual_hours * final_speedup_factor"
    ],
    "formula": "ai_hours = manual_hours * (base_speedup * size_multiplier * task_multiplier)",
    "constraints": [
      "Minimum speedup: 0.15 (never faster than 15% of manual)",
      "Maximum speedup: 0.90 (never faster than 10% of manual for safety)",
      "Reality check: If LOC > 10k, speedup should be > 0.50"
    ]
  },
  
